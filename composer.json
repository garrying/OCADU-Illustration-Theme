{
  "name": "garrying/ocaduillustration",
  "type": "package",
  "description": "A Wordpress thing for OCAD U Illustration Program.",
  "homepage": "https://github.com/garrying/OCADU-Illustration-Theme",
  "license": "MIT",
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "wptrt/wpthemereview": "^0.2.1"
  },
  "scripts": {
    "checkcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs --runtime-set testVersion 7.2-",
    "fixcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf --runtime-set testVersion 7.2-",
    "lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
  }
}